            <method name="open"><![CDATA[count = 0;

importPackage(Packages.java.util);
importPackage(Packages.java.lang);
importPackage(Packages.de.ingmbh.sphinx.online.common.vo);
importPackage(Packages.de.ingmbh.sphinx.online.service);

if (typeof eventService != 'undefined') {
	var creationTimeStart = null;
	var creationTimeStop = null;
	
	if (params["timestampFrom"] != undefined) {
		creationTimeStart = new Long(params["timestampFrom"]);
	}
	if (params["timePeriodTo"] != undefined) {
		creationTimeStop = new Long(params["timePeriodTo"]);
	}

	events = eventService.getEvents(params["projectLogicalName"], creationTimeStart, creationTimeStop, new Long(params["rowLimit"]));
} else {
	/* test data */
	events = new ArrayList();
	var event1 = new HashMap();
	event1.put("dbId", 1);
	event1.put("creationTime", 1396594260065);
	event1.put("description", "Description text");
	event1.put("eventType", "Event type");
	event1.put("severityType", "WARNING");
	event1.put("sourceId", "Source ID");
	event1.put("updateTime", 1396594360065);
	events.add(event1);
}]]></method>
            <method name="fetch"><![CDATA[if (count < events.size()) {
	var event = events.get(count);

	if (typeof eventService != 'undefined') {
		row["dbId"] = event.getDbId();
		row["creationTime"] = sooConversion.longToDate(event.getCreationTime());
		row["description"] = event.getDescription();
		row["eventType"] = event.getEventType();
		row["severityType"] = event.getSeverityType();
		row["sourceId"] = event.getSourceId();
		row["updateTime"] = sooConversion.longToDate(event.getUpdateTime());
	} else {
		/* test data */
		row["dbId"] = event.get("dbId");
		row["creationTime"] = sooConversion.longToDate(event.get("creationTime"));
		row["description"] = event.get("description");
		row["eventType"] = event.get("eventType");
		row["severityType"] = event.get("severityType");
		row["sourceId"] = event.get("sourceId");
		row["updateTime"] = sooConversion.longToDate(event.get("updateTime"));
	}
	
	count++;
	return true;
}

return false;]]></method>
